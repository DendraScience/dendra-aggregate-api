{
  "clients": {
    "stan": {
      "client": "build-api",
      "cluster": "test-cluster",
      "opts": {
        "maxPubAcksInflight": 1,
        "uri": "http://localhost:4222"
      }
    }
  },
  "databases": {
    "nedb": {
      "autocompactionInterval": 60000,
      "paginate": {
        "default": 20,
        "max": 2000
      },
      "path": "../.data"
    }
  },
  "host": "localhost",
  "port": 3034,
  "tasks": {
    "dispatch": {
      "requestSubject": "dendra.build.v1.req",
      "timerSeconds": [
        10,
        15
      ]
    },
    "grooming": {
      "docLimit": 20,
      "timerSeconds": [
        50,
        60
      ]
    },
    "stan": {
      "timerSeconds": [
        30,
        40
      ]
    }
  }
}